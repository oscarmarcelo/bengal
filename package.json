{
  "name": "generator-bengal",
  "version": "1.0.0-0",
  "description": "A scaffolding generator for front-end projects.",
  "main": "generators/app/index.js",
  "scripts": {
    "release": "np"
  },
  "files": [
    "generators",
    "templates"
  ],
  "keywords": [
    "yeoman-generator",
    "scaffold",
    "boilerplate",
    "template"
  ],
  "author": "Oscar Marcelo (https://oscarmarcelo.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oscarmarcelo/bengal.git"
  },
  "bugs": {
    "url": "https://github.com/oscarmarcelo/bengal/issues"
  },
  "homepage": "https://github.com/oscarmarcelo/bengal#readme",
  "dependencies": {
    "chalk": "^4.1.1",
    "figures": "^3.2.0",
    "generator-license": "^5.5.0",
    "get-port": "^5.1.1",
    "rimraf": "^3.0.2",
    "slugify": "^1.5.3",
    "yeoman-generator": "^5.3.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "np": "^7.5.0",
    "xo": "^0.39.1"
  },
  "xo": {
    "space": 2,
    "rules": {
      "no-multiple-empty-lines": [
        1,
        {
          "max": 2
        }
      ]
    },
    "ignores": [
      "templates/**/*"
    ]
  }
}
