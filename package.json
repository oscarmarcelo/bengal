{
  "name": "generator-bengal",
  "version": "1.0.0-7",
  "description": "A scaffolding generator for front-end projects.",
  "type": "module",
  "exports": "./generators/app/index.js",
  "engines": {
    "node": ">=18.14"
  },
  "scripts": {
    "lint": "xo",
    "release": "np"
  },
  "files": [
    "generators",
    "templates"
  ],
  "keywords": [
    "yeoman-generator",
    "scaffold",
    "boilerplate",
    "template"
  ],
  "author": "Oscar Marcelo (https://oscarmarcelo.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oscarmarcelo/bengal.git"
  },
  "bugs": {
    "url": "https://github.com/oscarmarcelo/bengal/issues"
  },
  "homepage": "https://github.com/oscarmarcelo/bengal#readme",
  "dependencies": {
    "@sindresorhus/slugify": "^2.2.1",
    "chalk": "^5.3.0",
    "configstore": "^6.0.0",
    "del": "^7.0.0",
    "email-validator": "^2.0.4",
    "figures": "^5.0.0",
    "generator-license": "^5.5.0",
    "get-port": "^7.0.0",
    "is-valid-hostname": "^1.0.2",
    "semver": "^7.5.4",
    "update-notifier": "^6.0.2",
    "validate-npm-package-name": "^5.0.0",
    "yeoman-generator": "^5.9.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "np": "^8.0.4",
    "xo": "^0.56.0"
  },
  "xo": {
    "space": 2,
    "rules": {
      "no-multiple-empty-lines": [
        "warn",
        {
          "max": 3
        }
      ],
      "import/order": [
        "warn",
        {
          "newlines-between": "always"
        }
      ]
    },
    "ignores": [
      "templates/**"
    ]
  },
  "np": {
    "tests": false,
    "message": "ðŸŽ‰ Release %s"
  }
}
