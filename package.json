{
  "name": "generator-bengal",
  "version": "1.0.0-3",
  "description": "A scaffolding generator for front-end projects.",
  "type": "module",
  "exports": "./generators/app/index.js",
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "lint": "xo",
    "release": "np"
  },
  "files": [
    "generators",
    "templates"
  ],
  "keywords": [
    "yeoman-generator",
    "scaffold",
    "boilerplate",
    "template"
  ],
  "author": "Oscar Marcelo (https://oscarmarcelo.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oscarmarcelo/bengal.git"
  },
  "bugs": {
    "url": "https://github.com/oscarmarcelo/bengal/issues"
  },
  "homepage": "https://github.com/oscarmarcelo/bengal#readme",
  "dependencies": {
    "@sindresorhus/slugify": "^2.1.0",
    "chalk": "^5.0.1",
    "figures": "^4.0.1",
    "generator-license": "^5.5.0",
    "get-port": "^6.1.2",
    "rimraf": "^3.0.2",
    "semver": "^7.3.7",
    "update-notifier": "^5.1.0",
    "validate-npm-package-name": "^4.0.0",
    "yeoman-generator": "^5.6.1",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "np": "^7.6.1",
    "xo": "^0.48.0"
  },
  "xo": {
    "space": 2,
    "rules": {
      "no-multiple-empty-lines": [
        1,
        {
          "max": 3
        }
      ]
    },
    "ignores": [
      "templates/**"
    ]
  },
  "np": {
    "tests": false,
    "message": "ðŸŽ‰ Release %s"
  }
}
